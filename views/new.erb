<form method="post" action="/students">
<label for="first-name-field">First Name:</label>
<input type="text"
 name="first_name"
 id="first-name-field"
 />

<label for="last-name-field">Last Name:</label>
<input type="text"
 name="last_name"
 id="last-name-field"
 />

<label for="house-selection">Select a House:</label>
<select name="house" id="house-selection">
  <option value="1">Griffindor</option>
  <option value="2">Hufflepuff</option>
  <option value="3">Ravenclaw</option>
  <option value="4">Slytherin</option>
</select>

<label for="age-field">Age:</label>
<input type="number"
 name="age"
 id="age-field"
 />

 <input type="submit" value="Create Student">

</form>
